<body style="margin: 0;">
  <ilw-page class="ilw-margin ilw-font">
    <ilw-header slot="header">
      <!-- Add header details here -->
    </ilw-header>
    <main>
      <h1>Redirecting, please wait...</h1>

      <ilw-content>
        <p>
          You will be redirected to <a href="^V">^U</a> in a few seconds.
          <br />
          If you are not redirected, <a href="^V">click here</a>

        </p>
        <br />
        <br />
      </ilw-content>
      <!-- Add content details here -->
    </main>
    <ilw-footer slot="footer">
      <!-- Add footer details here -->
    </ilw-footer>

    <script>
    navigator.sendBeacon('<%= "#{ENV['BEACON_BASE_URL']}/needhost" %>',
                         { 'url': '^V'})

     encodeURIComponent('^U'));

    setTimeout("window.location.href='^V'", 5000);
    </script>

</body>

